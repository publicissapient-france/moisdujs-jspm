{
  "name": "moisdujs-jspm",
  "version": "1.0.0",
  "description": "Packaging de lâ€™pplication de base avec JSPM pour le mois du JS",
  "scripts": {
    "serve": "http-server -a localhost -p 3000",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xebia-france/moisdujs-jspm"
  },
  "license": "ISC",
  "engines": {
    "node": "5.x.x",
    "npm": "3.x.x"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^0.9.0",
    "jspm": "^0.17.0-beta.14"
  },
  "jspm": {
    "name": "moisdujs-jspm",
    "main": "moisdujs-jspm.js",
    "directories": {
      "lib": "src"
    },
    "dependencies": {
      "bootstrap": "github:twbs/bootstrap@^3.3.6",
      "css": "github:systemjs/plugin-css@^0.1.21",
      "jquery": "npm:jquery@^2.2.4",
      "text": "github:systemjs/plugin-text@^0.0.8"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.10"
    },
    "overrides": {
      "github:twbs/bootstrap@3.3.6": {
        "shim": {
          "js/bootstrap": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "*"
        },
        "files": [
          "dist",
          "fonts",
          "js",
          "css",
          "less",
          "grunt",
          "LICENSE"
        ],
        "systemjs": {
          "main": "dist/js/bootstrap.js",
          "modules": {
            "dist/js/bootstrap.js": {
              "deps": [
                "jquery"
              ],
              "exports": "$"
            }
          }
        }
      },
      "npm:jquery@2.2.4": {
        "format": "amd"
      }
    }
  }
}
